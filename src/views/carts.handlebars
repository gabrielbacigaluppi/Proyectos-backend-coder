<h2>Detalle de productos en el carrito</h2>
<table>
    <thead>
        <tr>
            <th>Cantidad</th>
            <th>ID</th>
            <th>Nombre</th>
            <th>Descripción</th>
            <th>Código</th>
            <th>Precio</th>
            <th>Estado</th>
            <th>Stock</th>
            <th>Categoría</th>
        </tr>
    </thead>

    <tbody>

        {{#each productsArray}}
        {{#each this}}
        <tr>
            {{#if quantity}}
                <td>{{quantity}}</td>
            {{/if}}
            {{#each product_id}}
            {{#if this._id}}
                <td>{{this._id}}</td>
            {{/if}}
            {{#if this.title}}
                <td>{{this.title}}</td>
            {{/if}}
            {{#if this.description}}
                <td>{{this.description}}</td>
            {{/if}}
            {{#if this.code}}
             <td>{{this.code}}</td>
            {{/if}}
            {{#if this.price}}
                <td>{{this.price}}</td>
            {{/if}}
            {{#if this.status}}
                <td>{{this.status}}</td>
            {{/if}}
            {{#if this.stock}}
                <td>{{this.stock}}</td>
            {{/if}}
            {{#if this.category}}
                <td>{{this.category}}</td>
            {{/if}}
        
        {{/each}}
        
        </tr>
        {{/each}}
        {{/each}}
    </tbody>
</table>